{
  "name": "atomica",
  "version": "0.2.0-alpha.1",
  "type": "module",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": ["dist"],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    },
    "./dom": {
      "types": "./dist/dom/index.d.ts",
      "import": "./dist/dom/index.esm.js",
      "require": "./dist/dom/index.cjs.js"
    },
    "./signals": {
      "types": "./dist/signals/index.d.ts",
      "import": "./dist/signals/index.esm.js",
      "require": "./dist/signals/index.cjs.js"
    },
    "./dom/jsx-runtime": {
      "types": "./dist/dom/jsx-runtime.d.ts",
      "import": "./dist/dom/jsx-runtime.esm.js",
      "require": "./dist/dom/jsx-runtime.cjs.js"
    },
    "./dom/jsx-dev-runtime": {
      "types": "./dist/dom-jsx-runtime.d.ts",
      "import": "./dist/dom/jsx-runtime.esm.js",
      "require": "./dist/dom-jsx-runtime.cjs.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "@atomica/dom": "workspace:*",
    "@atomica/signals": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^8.5.1"
  }
}
